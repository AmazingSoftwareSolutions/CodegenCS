<?xml version="1.0"?>
<package >
  <metadata>
    <id>CodegenCS.DbSchema</id>
    <title>CodegenCS.DbSchema</title>
    <authors>Rick Drizin</authors>
    <owners>Rick Drizin</owners>
    <license type="expression">MIT</license>
    <projectUrl>https://github.com/Drizin/CodegenCS/tree/master/src/CodegenCS.DbSchema</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>This is a Plugin (Datasource Provider) to be used with CodegenCS code generator, which allows CodegenCS to generate code based on a Database Schema. Repository (Github) contains Code and Scripts to extract schema from SQL database into json files, and this schema can be used by any application.</description>
    <copyright>Copyright Rick Drizin 2019</copyright>
    <dependencies>
		<dependency id="CodegenCS" version="[1.0.6" include="All" />
    </dependencies>
  </metadata>
  <files>
    <file src="bin\$configuration$\CodegenCS.DbSchema.*" target="lib\net472" />
    <file src="bin\$configuration$\CodegenCS.DbSchema.*" target="lib\netstandard2.0" />
  </files>
</package>